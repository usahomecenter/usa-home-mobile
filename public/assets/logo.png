    <circle cx="220" cy="340" r="8" fill="white" />
    
    <!-- Windows -->
    <rect x="130" y="190" width="80" height="80" rx="5" fill="#E0F2FE" stroke="#4F46E5" stroke-width="5" />
    <rect x="290" y="190" width="80" height="80" rx="5" fill="#E0F2FE" stroke="#4F46E5" stroke-width="5" />
  </g>
  
  <!-- Text USA HOME -->
  <text x="256" y="430" text-anchor="middle" font-family="Arial, sans-serif" font-weight="bold" font-size="42" fill="#333333">USA HOME</text>
